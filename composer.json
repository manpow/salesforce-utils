{
    "name": "adamaveray/salesforce-utils",
    "description": "A collection of utilities to make interacting with the Phpforce Salesforce SOAP library easier",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/adamaveray/salesforce-utils",
    "authors": [
        {
            "name": "Adam Averay",
            "email": "adam@averay.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AdamAveray\\SalesforceUtils\\": "src/AdamAveray/SalesforceUtils/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "phpforce/soap-client": "dev-partner",
        "doctrine/collections": "^1.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/milesnash/soap-client.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.1@dev"
    }
}
